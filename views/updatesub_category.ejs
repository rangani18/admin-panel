
<%- include("header") %>
            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="card">
                                    <div class="card-header">
                                        <strong>Add sub-category</strong> Details
                                    </div>
                                    <div class="card-body card-block">
                                        <form action="/admin/sub_category/updatesub_categorydata" method="post" enctype="multipart/form-data" class="form-horizontal">
                                            <input type="hidden" value="<%= subdata.id %>" name="EditId">

                                            <% var city =["Surat","Navsari","Rajkot","Jamnagar"] %>
                                            <div class="row form-group">
                                                <div class="col col-md-3">
                                                    <label for="select" class=" form-control-label">Select City</label>
                                                </div>
                                                <div class="col-12 col-md-9">
                                                    <select name="city" id="select" class="form-control">
                                                        <option><%= subdata.city %></option>
                                                        <% city.map((v)=>{%>
                                                        <option value="<%= v %>"><%= v %></option>
                                                        <% }) %>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="row form-group">
                                                <div class="col col-md-3">
                                                    <label for="text-input" class=" form-control-label">Title</label>
                                                </div>
                                                <div class="col-12 col-md-9">
                                                    <input type="text" id="text-input" name="title" placeholder="Enter title" value="<%= subdata.title %>" class="form-control">
                                                </div>
                                            </div>
                                            
                                            <div class="row form-group">
                                                <div class="col col-md-3">
                                                    <label for="textarea-input" class=" form-control-label">Description</label>
                                                </div>
                                                <div class="col-12 col-md-9">
                                                    <textarea name="description" id="textarea-input" rows="3" placeholder="Description..." class="form-control"><%= subdata.description %></textarea>
                                                </div>
                                            </div>

                                            <div class="row form-group">
                                                <div class="col col-md-3">
                                                    <label for="file-input" class=" form-control-label">Upload Image</label>
                                                </div>
                                                <div class="col-12 col-md-9">
                                                    <input type="file" id="file-input" name="sub_category_image" class="form-control-file">
                                                    <img src="<%= subdata.sub_category_image %>" alt="">
                                                </div>
                                            </div>

                                            <!-- <div class="row form-group">
                                                <div class="col col-md-3">
                                                    <label for="file-multiple-input" class=" form-control-label">Multiple File input</label>
                                                </div>
                                                <div class="col-12 col-md-9">
                                                    <input type="file" id="file-multiple-input" name="file-multiple-input" multiple="" class="form-control-file">
                                                </div>
                                            </div> -->
                                            <div class="card-footer">
                                                <button type="submit" class="btn btn-primary btn-sm">
                                                    <i class="fa fa-dot-circle-o"></i> edit
                                                </button>
                                                <button type="reset" class="btn btn-danger btn-sm">
                                                    <i class="fa fa-ban"></i> Reset
                                                </button>
                                            </div>
                                        </form>
                                    </div>
                                   
                                </div>
                            </div>
                        </div>
                   
                        <%- include("footer") %>
                       